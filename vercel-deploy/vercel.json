{
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/full",
      "destination": "/api/index.js"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    }
  ]
}